config_version=4

[application]

config/name="Christmas Village Creator"
run/main_scene="res://menus/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
Global="*res://Global.gd"
PlaceableObjects="*res://game/PlaceableObjects.gd"
VillageData="*res://game/VillageData.gd"
VillageMap="*res://game/VillageMap.gd"

[display]

window/size/width=240
window/size/height=160
window/size/test_width=720
window/size/test_height=480
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

